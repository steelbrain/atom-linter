{
  "name": "atom-linter",
  "version": "4.6.0",
  "description": "Helper module for atom linter providers",
  "main": "lib/index.js",
  "scripts": {
    "test": "apm test",
    "watch": "ucompiler watch",
    "compile": "ucompiler go"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/AtomLinter/atom-linter.git"
  },
  "keywords": [
    "atom",
    "linter"
  ],
  "author": "steelbrain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtomLinter/atom-linter/issues"
  },
  "homepage": "https://github.com/AtomLinter/atom-linter#readme",
  "dependencies": {
    "consistent-env": "^1.0.1",
    "named-js-regexp": "^1.3.1",
    "sb-promisify": "^1.0.0",
    "tmp": "~0.0.28"
  },
  "devDependencies": {
    "eslint": "^2.2.0",
    "babel-eslint": "^5.0.0",
    "eslint-config-airbnb": "^6.0.2",
    "ucompiler": "^3.0.1",
    "ucompiler-plugin-babel": "^3.0.0",
    "ucompiler-plugin-newline": "^3.0.0",
    "babel-preset-steelbrain": "^1.0.0",
    "babel-preset-steelbrain-async": "^1.0.1",
    "babel-preset-steelbrain-flow": "^1.0.1"
  },
  "eslintConfig": {
    "rules": {
      "no-param-reassign": [
        2,
        {
          "props": false
        }
      ],
      "func-names": 0,
      "comma-dangle": 0,
      "no-extra-semi": 1,
      "no-loop-func": 0,
      "semi": [
        2,
        "never"
      ],
      "prefer-arrow-callback": 0
    },
    "extends": "airbnb/base",
    "parser": "babel-eslint",
    "globals": {
      "atom": "true"
    },
    "env": {
      "es6": true,
      "node": true
    }
  }
}
